#: ../libempathy/empathy-utils.c:304
msgid "Available"
msgstr "Có mặt"

#: ../libempathy/empathy-utils.c:306
msgid "Busy"
msgstr "Bận"

#: ../libempathy/empathy-utils.c:309
msgid "Away"
msgstr "Vắng mặt"

#: ../libempathy/empathy-utils.c:311
msgid "Invisible"
msgstr "Vô hình"

