msgid "Busy"
msgstr "Impegnât"

#: ../libempathy/empathy-utils.c:185
msgid "Away"
msgstr "Assent"

#: ../libempathy/empathy-utils.c:187
msgid "Invisible"
msgstr "Invisibil"

#: ../libempathy/empathy-utils.c:189
msgid "Offline"
msgstr "Fûr rêt"

